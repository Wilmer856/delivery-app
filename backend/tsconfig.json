{
    "compilerOptions": {
      "module": "CommonJS",
      "rootDir": "./src",
      // "moduleResolution": "NodeNext",
      "target": "ES2020",
      "sourceMap": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
    },
    "include": ["src/**/*"],
}